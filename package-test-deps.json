{
  "name": "firebase-notification-test-environment",
  "version": "1.0.0",
  "description": "Enterprise-grade Firebase notification testing environment for millions of concurrent users",
  "main": "firebase-notification-test-environment.js",
  "scripts": {
    "test:basic": "node firebase-notification-test-environment.js",
    "test:load": "node firebase-load-test-millions.js",
    "test:monitor": "node firebase-notification-monitor.js",
